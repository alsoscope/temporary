CREATE table dog(
	id int not null auto_increment primary key,
	kind varchar(12) not null,
	price int not null,
	image varchar(20) not null,
	nation varchar(12) not null,
	height int,
	weight int,
	content varchar(400),
	readcount int
);

alter table dog change nation country varchar(12);

